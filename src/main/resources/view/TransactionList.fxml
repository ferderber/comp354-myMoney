<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import main.java.controllers.TransactionListController?>

<ScrollPane xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="main.java.controllers.TransactionListController"
	styleClass="transaction-list" hbarPolicy="NEVER" >
	
	<GridPane alignment = "center" prefHeight="600.0" prefWidth="400.0" maxHeight="Infinity" maxWidth="Infinity">
        
		<children>
		<VBox fx:id="container" styleClass="transaction-container" GridPane.columnIndex="0" 
			GridPane.rowIndex="0" minWidth="200" minHeight="60" GridPane.hgrow="always" GridPane.vgrow="always">
			
		</VBox>

		<VBox fx:id="containerType" styleClass="transaction-container-2" GridPane.columnIndex="1" 
			GridPane.rowIndex="0" minWidth="200" minHeight="60" GridPane.hgrow="always" GridPane.vgrow="always">
			
		</VBox>
		</children>
	</GridPane>
	
</ScrollPane>

